{
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build --target es2022 --source-map --quiet src/main.ts",
    "build:ci": "ncc build --target es2022 --minify --quiet src/main.ts",
    "prerun": "npm run build",
    "run": "node dist/index.js",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@types/node": "16.11.45",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.20.0",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@actions/core": "1.9.0",
    "@actions/exec": "1.1.1",
    "fast-deep-equal": "3.1.3",
    "runtypes": "6.5.1"
  }
}
